    <style name="Theme.SkipFeed" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="colorPrimary">@color/focus_blue</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorPrimaryContainer">@color/focus_blue</item>
        <item name="colorOnPrimaryContainer">@color/white</item>
        
        <item name="colorSecondary">@color/focus_blue</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorSecondaryContainer">@color/background_secondary</item>
        <item name="colorOnSecondaryContainer">@color/text_primary</item>
        
        <item name="colorTertiary">@color/text_secondary</item>
        <item name="colorOnTertiary">@color/white</item>
        
        <item name="colorError">@color/error</item>
        <item name="colorOnError">@color/white</item>
        
        <item name="colorSurface">@color/background_primary</item>
        <item name="colorOnSurface">@color/text_primary</item>
        
        
        
        
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:navigationBarColor">@color/transparent</item>
        <item name="android:windowLightStatusBar">true</item>
        
        
    </style>
    <style name="Theme.SkipFeed.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/gradient_top</item>
        <item name="windowSplashScreenAnimatedIcon">@mipmap/ic_launcher_foreground</item>
        <item name="windowSplashScreenAnimationDuration">300</item>
        <item name="postSplashScreenTheme">@style/Theme.SkipFeed</item>
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:navigationBarColor">@color/transparent</item>
